            "type": "string",
            "minLength": 1,
            "maxLength": 2048,
            "nullable": true
          },
          "card_pan_length": {
            "type": "integer",
            "minimum": 1,
            "nullable": true
          },
          "card_category": {
            "type": "string",
            "minLength": 1,
            "maxLength": 2048,
            "nullable": true
          },
          "extra_data": {
            "allOf": [
              {
                "additionalProperties": true,
                "type": "object",
                "nullable": true
              },
              {
                "description": "Additional data associated with this Payment Method (limited to 4kB by default)"
              }
            ]
          }
        },
        "required": ["method"]
      },
