    "anyOf": [
      {
        "title": "Card Payment",
        "additionalProperties": false,
        "type": "object",
        "properties": {
          "method": {
            "type": "string",
            "description": "Determines the type of Payment Method - for card payments must be a  <code>card</code> string",
            "enum": ["card"]
          },
          "payment_gateway": {
            "type": "string",
            "minLength": 1,
            "maxLength": 64,
            "description": "Name of the payment gateway",
            "nullable": true,
            "example": "stripe"
          },
          "card_token": {
            "type": "string",
            "minLength": 1,
            "maxLength": 128,
            "description": "Tokenized credit card number, see <a href='/docs/integration_guide/faq#card_token'>card_token</a>",
            "nullable": true
          },
          "card_bin_number": {
            "type": "string",
            "pattern": "^[0-9]{6,9}$",
            "description": "Card BIN number",
            "nullable": true
          },
          "card_number_last4": {
            "type": "string",
            "pattern": "^[0-9]{4}$",
            "description": "Last four digits of credit card number",
            "nullable": true
          },
          "card_expiry_year": {
            "type": "integer",
            "minimum": 1900,
            "maximum": 2100,
            "description": "Card expiry year (as 4 digit number)",
            "nullable": true
          },
          "card_expiry_month": {
            "type": "integer",
            "minimum": 1,
            "maximum": 12,
            "description": "Card expiry month (1-12)",
            "nullable": true
