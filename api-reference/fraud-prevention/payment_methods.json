          "cardholder_name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 256,
            "description": "Cardholder's first and last name",
            "nullable": true
          },
          "cvv_result_code": {
            "type": "string",
            "description": "CVV verification response codes",
            "enum": ["M", "N", "P", "S", "U", "Empty", null],
            "nullable": true
          },
          "avs_result_code": {
            "type": "string",
            "minLength": 1,
            "maxLength": 1,
            "description": "Read more in: <a href=\"https://en.wikipedia.org/wiki/Address_Verification_System\">Address Verification System</a>",
            "nullable": true
          },
          "payment_category": {
            "type": "string",
            "description": "Represents the channel through which the user provided their card details. Options include: <ul>  <li>    <strong>e_commerce:</strong> The user provided their card details through an online platform or website, typically during a purchase made via a web or mobile application.  </li>  <li>    <strong>pos (Point of Sale):</strong> The card details were provided in person at a physical location, using a Point of Sale (POS) system such as a card reader or terminal.  </li>  <li>    <strong>moto (Mail Order/Telephone Order):</strong> The card details were provided remotely, either through mail or over the phone, where the cardholder was not physically present.  </li></ul>",
            "enum": ["e_commerce", "pos", "moto"],
            "nullable": true
          },
          "recurring_payment_phase": {
            "type": "string",
            "description": "Indicates the phase of a recurring payment. Options include: <ul><li><strong>Registration:</strong> Initial setup of the recurring payment.</li><li><strong>CIT (Customer Initiated Transaction):</strong> The customer actively authorizes the payment.</li><li><strong>MIT (Merchant Initiated Transaction):</strong> The payment is processed without direct customer action.</li></ul>",
            "enum": ["registration", "cit", "mit"],
            "nullable": true
          },
          "recurring_payment_reference": {
            "type": "string",
            "description": "A reference identifier for the recurring payment. This field is optional and should be provided if the payment is a recurring payment.",
            "minLength": 1,
            "maxLength": 128,
            "nullable": true,
            "example": "155172881"
          },
          "trusted_merchant": {
            "type": "boolean",
            "description": "Indicates whether the payment is made to a trusted merchant.",
            "nullable": true
          },
          "value": {
            "allOf": [
              {
                "additionalProperties": false,
                "type": "object",
                "properties": {
                  "amount": {
                    "oneOf": [
                      {
                        "type": "string",
                        "title": "string",
                        "description": "An amount of money as decimal string.",
                        "pattern": "^\\d+(\\.\\d+)?$",
                        "example": "0.99"
                      },
                      {
                        "type": "integer",
                        "title": "integer",
                        "description": "An amount of money as integer. Must be given in smallest possible unit according to ISO 4217. E.g. 1.99 USD = 199 (cents), 30 BRL = 3000 (centavos), 5 HUF = 500 (filler), 555 JPY = 555 (Yen), 44 BHD = 44000 (fils). This option is not available for cryptocurrency amount, in such a case please use the decimal string.",
                        "minimum": 0,
                        "example": 99
                      }
                    ]
                  },
                  "currency": {
                    "type": "string",
                    "description": "<a href=\"https://en.wikipedia.org/wiki/ISO_4217\">ISO 4217</a> currency code or cryptocurrency symbol.",
